	</servlet>

	<servlet-mapping>
		<servlet-name>SendIndex</servlet-name>
		<url-pattern>/SendIndex</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RegistAdminUser</servlet-name>
