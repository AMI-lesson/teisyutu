	</servlet>

	<servlet-mapping>
		<servlet-name>LoginChk</servlet-name>
		<url-pattern>/LoginChk</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>SendIndex</servlet-name>
